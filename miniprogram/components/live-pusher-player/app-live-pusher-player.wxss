.v-full {
    width: 100vw;
    height: 100vh;
}

.v-full2 {
    width: 100vw;
    height: 123.07vw;
}

.v-main-video {
    width: 100vw;
    height: 100vh;
}

.v-sub-video {
    width: 260rpx;
    height: 330rpx;
    position: relative;
}

.v-sub-video-list {
    position: absolute;
    display: flex;
    width: 260rpx;
    height: 330rpx;
    left: 20rpx;
    top: 160rpx;
    overflow: hidden;
    flex-direction: column-reverse;
}

.user-select-color {
    width:32rpx;
    height:32rpx;
    border:2rpx solid rgba(255,255,255,0.6);
    position: absolute;
    bottom: 10rpx;
    right: 6rpx;
    border-radius: 50%;
    overflow: hidden;
}

.v-border {
    border: 1px solid red;
}

.live-content {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}